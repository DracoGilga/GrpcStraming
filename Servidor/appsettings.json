{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "EndpontDefaults": {
            "protocols": "http2"
        }
    },
    "profiles":{
        "http":{
            "commandName": "Project",
            "dotnetRunMessages": "true",
            "launchBrowser": "true",
            "applicationUrl": "http://localhost:8080",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    }
}
